<archetype xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0 http://maven.apache.org/xsd/archetype-1.0.0.xsd">
    <id>quickstart</id>
    <sources>
        <source>src/main/java/App.java</source>
    </sources>    
    <resources>
        <resource>.gitignore</resource>
        <resource>README.md</resource>
        <resource>src/main/resources/logback.xml</resource>
    </resources>
    <testSources>
        <source>src/test/java/AppTest.java</source>
    </testSources>
    <testResources>
        <resource>src/test/resources/logback-test.xml</resource>
    </testResources>
</archetype>
